pipeline {
    agent any
    tools {
        maven "maven123"
    }
    options {
        timeout(30)
    }
    stages {
        stage('Checkout') {
            steps {
                checkout scm 
            }
        }
        stage('Build') {
            steps {
                sh 'mvn clean install'
            }
        }
        stage('Test') {
            steps {
                echo "display results "
            }
        }
    }
}
