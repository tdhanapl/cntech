pipeine {
    agent any
    tools {
        maven "maven123"
    }
    options {
        timestamp
        timeout(30)
    }
    stages {
        stage('Checkout') {
            steps {
                checkout scm
            }
        }

        stage('Build') {
            steps {
               sh  'maven clean install'
            }
        }
        
        stage('Test') {
            steps {
                echo " disply results"
            }
        }      
}
