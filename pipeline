pipeline {

agent {
        label 'agent1'
    }
    tools {
        maven "maven123"
    }
    options {
        timestamps
      
    }
    stages {
        stage('Checkout') {
            steps {
                checkout scm
            }
        }

        stage('Build') {
            steps {
                sh "mvn clean install"
            }
        }
    }
}
